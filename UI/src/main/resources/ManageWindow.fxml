<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="320.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.test.UIControllers.ManageWindowController">
    <children>
        <Button fx:id="addNeuron" layoutX="52.0" layoutY="70.0" mnemonicParsing="false" onMouseClicked="#add"
                prefHeight="26.0" prefWidth="97.0" text="Добавить"/>
        <Button fx:id="removeNeuron" layoutX="51.0" layoutY="132.0" mnemonicParsing="false" onMouseClicked="#remove"
                prefHeight="26.0" prefWidth="97.0" text="убрать" textAlignment="CENTER"/>
        <ChoiceBox fx:id="choiceBox" layoutX="25.0" layoutY="27.0" prefWidth="150.0"/>
        <Button fx:id="trainButton" layoutX="51.0" layoutY="164.0" mnemonicParsing="false"
                onAction="#onButtonTrainClick" prefHeight="26.0" prefWidth="97.0" text="Тренировать"/>
        <Button fx:id="viewNeuron" layoutX="51.0" layoutY="101.0" mnemonicParsing="false" onMouseClicked="#view"
                prefHeight="26.0" prefWidth="97.0" text="Посмотреть"/>
        <Label layoutX="20.0" layoutY="264.0" text="Текущая ошибка"/>
        <TextField fx:id="currentError" disable="true" layoutX="20.0" layoutY="281.0"/>
        <Button fx:id="stopButton" disable="true" layoutX="51.0" layoutY="195.0" mnemonicParsing="false"
                onAction="#onButtonStopClick" prefHeight="26.0" prefWidth="97.0" text="Остановить"/>
    </children>
</AnchorPane>
